PROGRAM MAIN
   READ*,A1,B1,C1,A2,B2,C2
   D=A1*B2-A2*B1
   X=(B2*C1-C2*B1)/D
   Y=(A1*C2-A2*C1)/D
   WRITE(6,12)A1,B1,C1,A2,B2,C2,X,Y
12 FORMAT(11X,'COFFIEIENTS OF LINERA EQUATIONS'//15X,'FIRST',3X,3F8.1//15X,
1        'SECOND',2X,3F8.1//11X,'SOLUTION OF LINEAR EQATION'//15X,'X=',F12.2//15X,'Y=',F12.2)

END PROGRAM
